package com.design.creational.factory;

public class CarFactory {

	public static Car buildCar(CarType type) {
		Car car = null;
		switch(type) {
		case SMALL:
			car = new SmallCar();
			break;
		case SEDAN:
			car = new SedanCar();
			break;
		case LUXURY:
			car = new LuxuryCar();
		default:
			break;
		
		}
		return car;
		
	}
}
